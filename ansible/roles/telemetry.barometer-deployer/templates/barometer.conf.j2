LoadPlugin amqp1
<Plugin amqp1>
<Transport "name">
    Host "{{ barometer_host }}"
    Port "{{ barometer_port }}"
    Address "collectd"
    <Instance "notify">
        Format JSON
        PreSettle true
        Notify true
    </Instance>
    <Instance "telemetry">
        Format JSON
        PreSettle false
    </Instance>
</Transport>
</Plugin>
